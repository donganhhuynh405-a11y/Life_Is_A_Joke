{
  "__inputs": [],
  "__requires": [
    {"type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0"},
    {"type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0"}
  ],
  "annotations": {"list": []},
  "description": "ML model performance metrics dashboard",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "id": 1,
      "title": "Model Accuracy",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "max": 1,
          "min": 0,
          "thresholds": {"mode": "absolute", "steps": [
            {"color": "red", "value": null},
            {"color": "yellow", "value": 0.5},
            {"color": "green", "value": 0.65}
          ]},
          "unit": "percentunit"
        }
      },
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 1},
      "id": 2,
      "options": {"minVizHeight": 75, "minVizWidth": 75, "orientation": "auto", "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}, "showThresholdLabels": false, "showThresholdMarkers": true},
      "title": "Model Accuracy by Model",
      "type": "gauge",
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "trading_bot_model_accuracy", "legendFormat": "{{model_name}} / {{symbol}}", "refId": "A"}
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {"color": {"mode": "palette-classic"}, "unit": "percentunit"}
      },
      "gridPos": {"h": 6, "w": 16, "x": 8, "y": 1},
      "id": 3,
      "options": {"legend": {"calcs": ["mean", "last"], "displayMode": "table", "placement": "right", "showLegend": true}, "tooltip": {"mode": "multi", "sort": "none"}},
      "title": "Model Accuracy Over Time",
      "type": "timeseries",
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "trading_bot_model_accuracy", "legendFormat": "{{model_name}} / {{symbol}}", "refId": "A"}
      ]
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 7},
      "id": 10,
      "title": "Inference Performance",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {"color": {"mode": "palette-classic"}, "unit": "s"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 11,
      "options": {"legend": {"calcs": ["mean", "max"], "displayMode": "table", "placement": "right", "showLegend": true}, "tooltip": {"mode": "multi", "sort": "none"}},
      "title": "Inference Latency (p50/p95/p99)",
      "type": "timeseries",
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "histogram_quantile(0.50, rate(trading_bot_model_inference_latency_seconds_bucket[5m]))", "legendFormat": "p50 {{model_name}}", "refId": "A"},
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "histogram_quantile(0.95, rate(trading_bot_model_inference_latency_seconds_bucket[5m]))", "legendFormat": "p95 {{model_name}}", "refId": "B"},
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "histogram_quantile(0.99, rate(trading_bot_model_inference_latency_seconds_bucket[5m]))", "legendFormat": "p99 {{model_name}}", "refId": "C"}
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {"color": {"mode": "palette-classic"}, "unit": "reqps"}
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 12,
      "options": {"legend": {"calcs": ["mean"], "displayMode": "list", "placement": "bottom", "showLegend": true}, "tooltip": {"mode": "single", "sort": "none"}},
      "title": "Prediction Throughput",
      "type": "timeseries",
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "rate(trading_bot_model_predictions_total[5m])", "legendFormat": "{{model_name}} / {{symbol}}", "refId": "A"}
      ]
    },
    {
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
      "id": 20,
      "title": "Prediction Volume",
      "type": "row"
    },
    {
      "datasource": {"type": "prometheus", "uid": "${datasource}"},
      "fieldConfig": {
        "defaults": {"color": {"mode": "palette-classic"}, "unit": "short"}
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 17},
      "id": 21,
      "options": {"legend": {"calcs": ["sum"], "displayMode": "table", "placement": "right", "showLegend": true}, "tooltip": {"mode": "multi", "sort": "none"}},
      "title": "Total Predictions by Model",
      "type": "timeseries",
      "targets": [
        {"datasource": {"type": "prometheus", "uid": "${datasource}"}, "expr": "increase(trading_bot_model_predictions_total[1h])", "legendFormat": "{{model_name}} / {{symbol}}", "refId": "A"}
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["trading", "ml", "models"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource",
        "label": "Prometheus"
      },
      {
        "current": {},
        "datasource": {"type": "prometheus", "uid": "${datasource}"},
        "definition": "label_values(trading_bot_model_accuracy, model_name)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "model",
        "options": [],
        "query": {"query": "label_values(trading_bot_model_accuracy, model_name)", "refId": "StandardVariableQuery"},
        "refresh": 2,
        "sort": 1,
        "type": "query",
        "label": "Model"
      }
    ]
  },
  "time": {"from": "now-3h", "to": "now"},
  "timepicker": {},
  "timezone": "browser",
  "title": "ML Model Performance",
  "uid": "ml-performance",
  "version": 1
}
